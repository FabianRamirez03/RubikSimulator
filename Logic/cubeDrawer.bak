             (cube (pos 0 1/4 1/4) 1/8))
 (with-color (rgba "white" 0)
             (cube (pos 1/4 0 1/4) 1/8))
 (with-color (rgba "white")
             (cube (pos -1/4 1/4 1/4) 1/8))
 ))
