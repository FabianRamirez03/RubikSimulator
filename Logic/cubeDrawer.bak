;;el de mentiritas fijo
(define cubo (combine(with-color (rgba "red")
             (cube origin 1/8))
 (with-color (rgba "green")
             (cube (pos 0 0 1/4) 1/8))
 (with-color (rgba "yellow")
             (cube (pos 0 1/4 0) 1/8))
